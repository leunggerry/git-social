<html lang="en">
  <head>
    <meta content="charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Git Social</title>
    <link rel="icon" type="image/x-icon" href="/images/git-social-logo.png">
    <!-- CSS  -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <link
      href="/stylesheets/materialize.css"
      type="text/css"
      rel="stylesheet"
      media="screen,projection"
    />
    <link
      href="/stylesheets/style.css"
      type="text/css"
      rel="stylesheet"
      media="screen,projection"
    />
  </head>

  <body
    class="logged-out page-responsive header-overlay tab-size-and-alignment-fix"
    style="word-wrap: break-word"
  >
    <div class="position-relative">
      <header
        class="Header-old header-logged-out js-details-container Details position-relative f4 py-2"
        role="banner"
      >
        <nav class="nav-wrapper container">
          {{! TODO: this should link to home }}
          <a id="logo-container" href="/" class="brand-logo white-text">Git
            Social</a>
          <div class="mt-0 px-3 px-lg-0 mb-5 mb-lg-0">
            <div class="container-xl d-lg-flex flex-items-center p-responsive">
              <div
                class="HeaderMenu HeaderMenu--logged-out position-fixed top-0 right-0 bottom-0 height-fit position-lg-relative d-lg-flex flex-justify-between flex-items-center flex-auto"
              >
                <!-- List items on dashboard -->
                <!-- Keep spacing on nav-->
                <nav class="mt-0 px-3 px-lg-0 mb-5 mb-lg-0" aria-label="Global">
                  {{! <ul class="d-lg-flex list-style-none hide-on-med-and-down">
                    <li
                      class="mr-0 mr-lg-3 position-relative flex-wrap flex-justify-between flex-items-center border-bottom border-lg-bottom-0 d-block d-lg-flex flex-lg-nowrap flex-lg-items-center"
                    ></li>
                  </ul> }}
                </nav>
                <div
                  class="d-lg-flex flex-items-center px-3 px-lg-0 text-center text-lg-left"
                >
                  {{#if loggedIn}}
                    <div
                      class="position-relative mr-3 mb-4 mb-lg-0 d-inline-block hide-on-med-and-down"
                    >
                      <ul
                        class="d-lg-flex list-style-none hide-on-med-and-down"
                      >
                        <li class="nav-item">
                          <a href="/">Home</a>
                        </li>
                        <li class="nav-item">
                          <a href="/dashboard">Dashboard</a>
                        </li>
                        <li class="nav-item">
                          <a
                            class="waves-effect waves-light btn-small logout-button"
                            id="logout"
                          >Logout
                            <svg
                              width="15"
                              height="15"
                              viewBox="0 0 15 15"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            ><path
                                d="M3 1C2.44771 1 2 1.44772 2 2V13C2 13.5523 2.44772 14 3 14H10.5C10.7761 14 11 13.7761 11 13.5C11 13.2239 10.7761 13 10.5 13H3V2L10.5 2C10.7761 2 11 1.77614 11 1.5C11 1.22386 10.7761 1 10.5 1H3ZM12.6036 4.89645C12.4083 4.70118 12.0917 4.70118 11.8964 4.89645C11.7012 5.09171 11.7012 5.40829 11.8964 5.60355L13.2929 7H6.5C6.22386 7 6 7.22386 6 7.5C6 7.77614 6.22386 8 6.5 8H13.2929L11.8964 9.39645C11.7012 9.59171 11.7012 9.90829 11.8964 10.1036C12.0917 10.2988 12.4083 10.2988 12.6036 10.1036L14.8536 7.85355C15.0488 7.65829 15.0488 7.34171 14.8536 7.14645L12.6036 4.89645Z"
                                fill="currentColor"
                                fill-rule="evenodd"
                                clip-rule="evenodd"
                              ></path></svg>
                          </a>

                        </li>
                      </ul>
                    </div>
                  {{else}}
                    <div
                      class="position-relative mr-3 mb-4 mb-lg-0 d-inline-block hide-on-med-and-down"
                    >
                      <a href="/login">Sign in</a>
                    </div>

                    <div
                      class="position-relative mr-3 mb-4 mb-lg-0 d-inline-block hide-on-med-and-down"
                    >
                      <form
                        data-turbo="false"
                        class="mx-auto mx-md-0 col-5-max js-signup-form position-relative z-2"
                        autocomplete="off"
                        action="/sign-up"
                        accept-charset="UTF-8"
                        method="get"
                      >
                        <div
                          class="d-flex flex-column flex-sm-row flex-items-center"
                        >
                          <button
                            class="btn-mktg width-full width-sm-auto btn-signup-mktg"
                            type="submit"
                          >
                            Sign up for Git Social
                          </button>
                        </div>
                      </form>
                    </div>
                  {{/if}}
                </div>
              </div>
              <!-- Nav on mobile -->
              <ul id="nav-mobile" class="sidenav">
                <li><a href="#">Sign in</a></li>
                <li><a href="#">Sign Up</a></li>
              </ul>
              <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i
                  class="material-icons"
                >menu</i></a>
            </div>
          </div>
        </nav>
      </header>
    </div>

    <main class="font-mktg">
      {{#unless loggedIn}}
        <!-- Welcome Section -->
        {{>welcome-page}}
      {{/unless}}
      {{{body}}}
    </main>

